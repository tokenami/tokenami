{
  "name": "@tokenami/cli",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tokenami": "bin.js"
  },
  "files": [
    "dist",
    "bin.js",
    "tokenami.config.js"
  ],
  "scripts": {
