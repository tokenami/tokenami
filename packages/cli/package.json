    "dev": "pnpm build-fast --watch src"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@stitches/stringify": "^1.2.8",
    "cac": "^6.7.14",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "csstype": "^3.1.2",
    "deepmerge": "^4.3.1",
    "fast-glob": "^3.2.12",
    "lightningcss": "^1.21.0"
  }
}
