{
  "name": "@tokenami/css",
  "version": "0.0.80",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/tokenami/tokenami",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@tokenami/config": "workspace:*"
  },
  "devDependencies": {
    "tokenami": "workspace:*",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "tokenami": ">= 0",
    "typescript": ">= 5"
  }
}
