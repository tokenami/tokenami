{
  "name": "@tokenami/eslint-plugin-dev",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "scripts": {
    "build": "tsup --format=esm,cjs src/index.ts",
    "build-fast": "pnpm build",
    "test": "jest",
    "update-docs": "pnpm build && eslint-doc-generator"
  },
  "dependencies": {
    "@tokenami/config": "workspace:*",
    "@typescript-eslint/parser": "^5.60.0",
    "@typescript-eslint/utils": "^5.60.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "eslint-doc-generator": "^1.4.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "eslint": ">= 8"
  }
}
